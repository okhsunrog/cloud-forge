---
# ===================
# WireGuard Keys
# ===================

# Moscow wg0 (clients server - serves NAS + devices)
# Also used for AmneziaWG (awg0)
vault_wg_moscow_clients_private: "server_private_key"
vault_wg_moscow_clients_public: "server_public_key"

# Moscow wg1 (upstream client - connects to Europe)
vault_wg_moscow_upstream_private: "moscow_upstream_private_key"
vault_wg_moscow_upstream_public: "moscow_upstream_public_key"

# Europe wg0 (upstream server - accepts Moscow)
vault_wg_europe_upstream_private: "europe_upstream_private_key"
vault_wg_europe_upstream_public: "europe_upstream_public_key"

# ===================
# WireGuard Peers
# ===================

# Clients that connect to Moscow wg0 and awg0
# WireGuard uses 10.66.66.X, AmneziaWG uses 10.65.65.X (same last octet)
# trusted: true = your devices (full access), false = friends (internet only, isolated)
vault_wg_peers:
  - name: "mira"
    private: "peer_private_key"
    public: "peer_public_key"
    ip: "10.66.66.2"
    trusted: true
  - name: "friend_device"
    private: "peer_private_key"
    public: "peer_public_key"
    ip: "10.66.66.5"
    trusted: false

# ===================
# OpenConnect Users
# ===================

ocserv_users:
  personal:
    - username: "user1"
      password: "password1"
  friends:
    - username: "friend1"
      password: "password1"

# ===================
# Coturn
# ===================

coturn_static_secret: "generate_with_openssl_rand_hex_32"
